<title>The Home Page</title>
<% if current_user %>
<h1>Welcome <% current_user.name %></h1>
<% show_user_posts.each do |post| %>
<div class="row">
	<strong><%=post.title %></strong>
	<p><%=post.author %></p>
	<p><%=post.body %></p>
	<p><%=post.d_type %></p>
</div>
<% end %>
<% else %>
<%=link_to "Login",login_url%>
<%=link_to 'SignUp',user_new_url %>
<% end %>